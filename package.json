{
  "name": "middy-monorepo",
  "version": "1.0.0-alpha.26",
  "description": "ğŸ›µ The stylish Node.js middleware engine for AWS Lambda",
  "engines": {
    "node": ">=6.10"
  },
  "engineStrict": true,
  "scripts": {
    "install": "lerna bootstrap",
    "test": "npm run test:lint && npm run test:packages",
    "test:lint": "eslint .",
    "test:packages": "lerna exec --bail --concurrency 1 npm run test",
    "release:tag": "git tag $npm_package_version && git push --tags",
    "lerna:sync": "lerna publish --yes --skip-npm --skip-git --repo-version $npm_package_version",
    "lerna:publish": "lerna publish --yes --skip-git --repo-version $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/middyjs/middy.git"
  },
  "keywords": [
    "Lambda",
    "Middleware",
    "Serverless",
    "Framework",
    "AWS",
    "AWS Lambda"
  ],
  "author": "Luciano Mammino, Peter Caulfield, Joe Minichino, Domagoj Katavic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/middyjs/middy/issues"
  },
  "homepage": "https://github.com/middyjs/middy#readme",
  "devDependencies": {
    "aws-sdk": "^2.418.0",
    "babel-jest": "^24.3.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "regenerator-runtime": "^0.13.1",
    "typings-tester": "^0.3.2"
  },
  "dependencies": {
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  }
}
